 "user_description"] 
 [LONGDESCRIPTION "long_description"] 
 [NTLINK "nt_user_id" 
 | NTSID "windows_user_ntsid"] 
 [PASSWORD "user_password"] 
 [LDAPLINK "LDAP_user_name"] 
 [TRUSTEDLOGIN "trusted_authenticated_id"] 
 [WHLINK "warehouse_login"] 
